smallBoardHeaderDiv()
  display: inline-block
  vertical-align: top
  height: 33px
  border-radius: 3px
  font-size: 12px
  box-sizing: border-box
  padding: 6px
 
darkBlueHover()
  background-color: #1D507A

greyBtnHover()
  background-color: #cdcdcd
  color: #666
  
navhover()
  background-color: #6EA2C0
  color: white
  
boardObj()
  width: auto
  height: 97px
  background: #0067A3
  border-radius: 3px
  padding: 10px
  position: relative

greenButton()
  background-color: #5FB74B
  color: white
  font-weight: bold
  width: 120px
  margin: 10px 0 0 0
  &:hover
    background-color: #4EA63A
